<!DOCTYPE html>
<html>
<head>
</head>
<body>

  <header>
  </header>

  <main>

    <h1>Game Listing</h1>

    <h3>Moves: <%= moves %></h3>
    <h3>Winner: <%= winner %></h3>
    <h3>Results: <%= games.length %></h3>

    <table>
      <tr>
        <th>Game</th>
        <th>Moves</th>
        <th>Winner</th>
      </tr>
      <% games.forEach( function(game) { %>
        <tr>
          <td><%= game.id %></td>
          <td><%= game.moves %></td>
          <td><%= game.winner %></td>
        </tr>
      <% }); %>
    </table>
  </main>

  <footer>
  </footer>

</body>
</html>